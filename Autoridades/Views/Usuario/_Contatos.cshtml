@model Autoridades.Models.Contatos
    <div class="col-xs-12">
        <div class="col-md-12">
            <h3> Passo 3 - Contatos</h3>
            <div class="form-group">
                <label class="control-label">Nome:</label>
                @Html.TextBoxFor(model => model.Name, new { @class = "form-control", RequiredAttributeAdapter = true })
                @Html.ValidationMessageFor(model => model.Name)
            </div>
            <div class="form-group">
                <label class="control-label">E-mail:</label>
                @Html.TextBoxFor(model => model.Email, new { @class = "form-control", RequiredAttributeAdapter = true })
                @Html.ValidationMessageFor(model => model.Email)
            </div>
            <div class="form-group">
                <label class="control-label">Complemento:</label>
                @Html.TextBoxFor(model => model.Complement, new { @class = "form-control", RequiredAttributeAdapter = true })
                @Html.ValidationMessageFor(model => model.Complement)
            </div>
            <div class="form-group">
                <label class="control-label">Telefone:</label>
                @Html.TextBoxFor(model => model.PhoneNumber, new { @class = "form-control", RequiredAttributeAdapter = true })
                @Html.ValidationMessageFor(model => model.PhoneNumber)
            </div>
            <button class="btn btn-success nextBtn btn-lg pull-right" type="button">Salvar informações</button>
        </div>
    </div>
