@model System.Collections.Generic.List<Autoridades.Models.Deputado>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">

            <div class="ibox-title">
                <h5>Deputados</h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                        <i class="fa fa-plus"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">

                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover dataTables-example">
                        <thead>
                            <tr>
                                <th>Matrícula</th>
                                <th>ID Parlamentar</th>
                                <th>Nome</th>
                                <th>Nome parlamentar</th>
                                <th>UF</th>
                                <th>Partido</th>
                                <th>Gabinete</th>
                                <th>Telefone</th>
                                <th>E-mail</th>

                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var deputado in Model)
                            {
                                <tr class="gradeX">
                                    <td>@deputado.Matricula</td>
                                    <td>
                                        @deputado.IdParlamentar
                                    </td>
                                    <td>@deputado.Nome</td>
                                    <td>@deputado.NomeParlamentar</td>
                                    <td>
                                        @deputado.Uf
                                    </td>
                                    <td>@deputado.Partido</td>
                                    <td>@deputado.Gabinete</td>
                                    <td>
                                        @deputado.Fone
                                    </td>
                                    <td>@deputado.Email</td>
                                </tr>
                            }


                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Matrícula</th>
                                <th>ID Parlamentar</th>
                                <th>Nome</th>
                                <th>Nome parlamentar</th>
                                <th>UF</th>
                                <th>Partido</th>
                                <th>Gabinete</th>
                                <th>Telefone</th>
                                <th>E-mail</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>

            </div>
        </div>
    </div>
</div>
